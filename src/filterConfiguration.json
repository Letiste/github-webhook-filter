{
  "$schema": "../schemas/filter_configuration.schema.json",
  "users_black_listed": ["dependabot[bot]"],
  "pull_request": {
    "permittedActions": ["closed", "opened", "reopened"]
  },
  "issues": {
    "permittedActions": ["opened", "closed", "deleted", "reopened"]
  },
  "create": {
    "permittedRefTypes": ["branch", "tag"]
  },
  "member": {
    "permittedActions": ["added", "edited", "removed"]
  },
  "deploy_key": {
    "permittedActions": ["created", "deleted"]
  },
  "repository": {
    "permittedActions": ["created", "deleted", "edited", "archived", "privatized", "publicized", "renamed", "transferred"]
  },
  "repository_vulnerability_alert": {
    "permittedActions": ["create", "resolve", "dismiss"]
  },
  "team_add": {
    "allowedAll": true
  },
  "code_scanning_alert": {
    "permittedActions": ["reopened"]
  },
  "membership": {
    "permittedActions": ["added", "removed"]
  },
  "milestone": {
    "permittedActions": ["created", "closed", "deleted", "edited", "opened"]
  },
  "organization": {
    "permittedActions": ["deleted", "member_added", "member_invited", "member_removed", "renamed"]
  },
  "release": {
    "permittedActions": ["created", "deleted", "edited", "prereleased", "published", "released", "unpublished"]
  },
  "secret_scanning_alert": {
    "permittedActions": ["created", "reopened", "resolved"]
  },
  "team": {
    "permittedActions": ["created", "deleted", "edited", "removed_from_repository", "added_to_repository"]
  }
}
