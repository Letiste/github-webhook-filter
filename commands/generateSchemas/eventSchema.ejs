{
  "type": "object",
  "properties": {
    "<%- eventName %>": {
      "allOf": [
        {
          "oneOf": [
            {
              "type": "object",
              "required": ["permitted<%- type %>s"],
              "properties": {
                "permitted<%- type %>s": {
                  "description": "White listed <%- type %>s",
                  "$ref": "#/definitions/<%- type %>sAvailable"
                }
              }
            },
            {
              "type": "object",
              "required": ["filtered<%- type %>s"],
              "properties": {
                "filtered<%- type %>s": {
                  "description": "Black listed <%- type %>s",
                  "$ref": "#/definitions/<%- type %>sAvailable"
                }
              }
            },
            {
              "type": "object",
              "required": ["allowedAll"],
              "properties": {
                "allowedAll": {
                  "description": "If true, allow every <%- type %>s",
                  "type": "boolean"
                }
              }
            }
          ]
        },
        {
          "type": "object",
          "properties": { "users_black_listed": { "type": "array", "items": { "type": "string" } } }
        }
      ]
    }
  },
  "definitions": {
    "<%- type %>sAvailable": {
      "type": "array",
      "items": {
        "enum": <%- JSON.stringify(eventTypes) %> 
      },
      "uniqueItems": true
    }
  }
}
