{
  "$id": "team.schema.json",
  "type": "object",
  "required": ["action", "sender"],
  "properties": {
    "action": {
        "enum": [
          "created",
          "deleted",
          "edited",
          "added_to_repository",
          "removed_from_repository"
        ]
    },
    "sender": {
      "type": "object",
      "required": ["login"],
      "properties": {
        "login": {
          "type": "string"
        }
      }
    }
  }
}
