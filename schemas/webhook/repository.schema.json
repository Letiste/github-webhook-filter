{
  "$id": "repository.schema.json",
  "type": "object",
  "required": ["action", "sender"],
  "properties": {
    "action": {
        "enum": [
          "created",
          "deleted",
          "archived",
          "unarchived",
          "edited",
          "renamed",
          "transferred",
          "publicized",
          "privatized"
        ]
    },
    "sender": {
      "type": "object",
      "required": ["login"],
      "properties": {
        "login": {
          "type": "string"
        }
      }
    }
  }
}
