{
  "$id": "organization.schema.json",
  "type": "object",
  "required": ["action", "sender"],
  "properties": {
    "action": {
        "enum": [
          "deleted",
          "renamed",
          "member_added",
          "member_removed",
          "member_invited"
        ]
    },
    "sender": {
      "type": "object",
      "required": ["login"],
      "properties": {
        "login": {
          "type": "string"
        }
      }
    }
  }
}
