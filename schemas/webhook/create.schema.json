{
  "$id": "create.schema.json",
  "type": "object",
  "required": ["ref_type", "sender"],
  "properties": {
    "ref_type": {
        "enum": [
          "branch",
          "tag"
        ]
    },
    "sender": {
      "type": "object",
      "required": ["login"],
      "properties": {
        "login": {
          "type": "string"
        }
      }
    }
  }
}
